<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="290dp"
    android:layout_height="290dp"
    android:background="@color/white">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/make_room_guide1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.893"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/make_room_guide2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.042"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/make_room_guide3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.96"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/make_room_guide4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.110"/>

    <ImageButton
        android:id="@+id/ib_make_room_off"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/make_room_guide2"
        app:layout_constraintBottom_toBottomOf="@id/make_room_guide4"
        app:layout_constraintStart_toStartOf="@id/make_room_guide1"
        app:layout_constraintEnd_toEndOf="@id/make_room_guide3"
        android:background="@null"
        android:src="@drawable/cancel"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/make_room_guide5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.17"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/make_room_guide6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.138"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/make_room_guide7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.831"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/make_room_guide8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.231"/>

    <TextView
        android:id="@+id/tv_make_room_title"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/make_room_guide6"
        app:layout_constraintBottom_toBottomOf="@id/make_room_guide8"
        app:layout_constraintStart_toStartOf="@id/make_room_guide5"
        app:layout_constraintEnd_toEndOf="@id/make_room_guide7"
        android:text="방 이름을 정해주세요"
        android:textSize="20sp"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/make_room_guide9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.350"/>

    <EditText
        android:id="@+id/et_make_room_title"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/make_room_guide8"
        app:layout_constraintBottom_toBottomOf="@id/make_room_guide9"
        app:layout_constraintStart_toStartOf="@id/make_room_guide5"
        app:layout_constraintEnd_toEndOf="@id/make_room_guide7"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/make_room_guide10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.400"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/make_room_guide11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.500"/>

    <TextView
        android:id="@+id/tv_make_room_member"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/make_room_guide10"
        app:layout_constraintBottom_toBottomOf="@id/make_room_guide11"
        app:layout_constraintStart_toStartOf="@id/make_room_guide5"
        app:layout_constraintEnd_toEndOf="@id/make_room_guide7"
        android:text="방 인원을 정해주세요"
        android:textSize="20sp"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/make_room_guide12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.600"/>

    <androidx.appcompat.widget.AppCompatSeekBar
        android:id="@+id/sb_make_room"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/make_room_guide11"
        app:layout_constraintBottom_toBottomOf="@id/make_room_guide12"
        app:layout_constraintStart_toStartOf="@id/make_room_guide5"
        app:layout_constraintEnd_toEndOf="@id/make_room_guide7"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/make_room_guide13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.273"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/make_room_guide14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.796"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/make_room_guide15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.723"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/make_room_guide16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.927"/>

    <Button
        android:id="@+id/button_make_room_add"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/make_room_guide14"
        app:layout_constraintBottom_toBottomOf="@id/make_room_guide16"
        app:layout_constraintStart_toStartOf="@id/make_room_guide13"
        app:layout_constraintEnd_toEndOf="@id/make_room_guide15"
        android:text="방 생성하기"
        android:background="#d75c4a"
        android:textColor="@color/white"/>

</androidx.constraintlayout.widget.ConstraintLayout>